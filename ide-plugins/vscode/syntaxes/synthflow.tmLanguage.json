{
    "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
    "name": "SynthFlow",
    "patterns": [
        {
            "include": "#keywords"
        },
        {
            "include": "#strings"
        },
        {
            "include": "#comments"
        },
        {
            "include": "#numbers"
        },
        {
            "include": "#operators"
        },
        {
            "include": "#identifiers"
        }
    ],
    "repository": {
        "keywords": {
            "patterns": [
                {
                    "name": "keyword.control.synthflow",
                    "match": "\\b(fn|let|if|else|while|return|type|struct|enum|intent|true|false|break|continue|for|array|import|export|class|match|async|await|try|catch|throw)\\b"
                },
                {
                    "name": "keyword.other.ai.synthflow",
                    "match": "\\b(gemini_complete|gemini_chat|set_gemini_api_key|has_gemini_api_key|agent|model|train|predict|evaluate|pipeline)\\b"
                },
                {
                    "name": "keyword.other.quantum.synthflow",
                    "match": "\\b(qubit|qreg|creg|hadamard|cnot|measure|pauliX|pauliY|pauliZ|phase|tgate|swap|ccnot|circuit)\\b"
                },
                {
                    "name": "storage.type.synthflow",
                    "match": "\\b(Int|Float|String|Bool|Tensor|Array|Map|Set|Option|Result|Void)\\b"
                }
            ]
        },
        "strings": {
            "patterns": [
                {
                    "name": "string.quoted.double.synthflow",
                    "begin": "\"",
                    "end": "\"",
                    "patterns": [
                        {
                            "name": "constant.character.escape.synthflow",
                            "match": "\\\\."
                        }
                    ]
                }
            ]
        },
        "comments": {
            "patterns": [
                {
                    "name": "comment.line.number-sign.synthflow",
                    "match": "#.*$"
                }
            ]
        },
        "numbers": {
            "patterns": [
                {
                    "name": "constant.numeric.float.synthflow",
                    "match": "\\b\\d+\\.\\d+\\b"
                },
                {
                    "name": "constant.numeric.integer.synthflow",
                    "match": "\\b\\d+\\b"
                }
            ]
        },
        "operators": {
            "patterns": [
                {
                    "name": "keyword.operator.synthflow",
                    "match": "(\\-\\>|\\=\\=|\\!\\=|\\<\\=|\\>\\=|\\&\\&|\\|\\||\\+|\\-|\\*|\\/|\\%|\\=|\\!|\\<|\\>|\\&|\\|)"
                }
            ]
        },
        "identifiers": {
            "patterns": [
                {
                    "name": "variable.other.synthflow",
                    "match": "\\b[a-zA-Z_][a-zA-Z0-9_]*\\b"
                }
            ]
        }
    },
    "scopeName": "source.synthflow"
}