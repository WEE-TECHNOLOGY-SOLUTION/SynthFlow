# Gemini API Test
# This test demonstrates the native Gemini API integration
# IMPORTANT: Replace YOUR_API_KEY with your actual Gemini API key

print("=== Gemini API Test ===")
print("")

# Step 1: Set your API key
# Get your key from: https://ai.google.dev
let API_KEY = "YOUR_API_KEY"  # Replace with your actual key

print("Setting API key...")
gemini_set_api_key(API_KEY)

# Step 2: Check if key is set
if (gemini_has_api_key()) {
    print("API key is configured!")
} else {
    print("Warning: API key not set")
}
print("")

# Step 3: Simple completion test
print("=== Test 1: Simple Completion ===")
let response = gemini_complete("What is 2 + 2? Answer in one word.")
print("Response:", response)
print("")

# Step 4: Chat with system instruction
print("=== Test 2: Chat with System Instruction ===")
let chatResponse = gemini_chat(
    "You are a helpful math tutor. Be concise.",
    "What is the square root of 144?"
)
print("Chat Response:", chatResponse)
print("")

# Step 5: Test with agent-like system prompt
print("=== Test 3: Agent-style Chat ===")
let agentResponse = gemini_chat(
    "You are a coding assistant. Help users with programming questions. Be brief and provide code examples when helpful.",
    "How do I print Hello World in Python?"
)
print("Agent Response:", agentResponse)
print("")

print("=== All Tests Complete ===")
